Data = record {
    id: i64 ;
    value: string ;
}

Action = enum {
    Insert;
    Remove;
    Ignore;
}

Logger = interface +j +o  {
    write(action: i64, id:i64): bool;
}


Storage = interface +c {

    const get(id:i64): optional<Data>;

    add(data: Data): bool ;

    remove(id:i64): bool ;

    const size () : i64 ;

    static create(logger: Logger) : Storage;
}



